    "print(\"Shape matches:\", df.shape == df_verify.shape)\n",
    "print(\"DataFrames are equal:\", df.equals(df_verify))\n",
    "\n",
    "df.head()"
    "df = readAllFilesInDirectoryToDataFrame(\"data\")\n",
    "print(df)"
